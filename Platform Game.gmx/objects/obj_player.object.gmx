     jumping = true;
    }
    else
    jumping = false;
    
    if abilitystate = 1 &amp;&amp; instance_exists(obj_iceability)
    {
        jumping = false;
    }
}

if input_jump_released &amp;&amp; vsp &lt; 0
{
 vsp *= 0.5;
}
